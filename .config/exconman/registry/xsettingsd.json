[
	{
		"name": "xsettingsd.FontName",
		"file": "~/.config/xsettingsd/xsettingsd.conf",
		"pattern": "Gtk/FontName .+",
		"substitute": "Gtk/FontName \"{value}\"",
		"replace": "matched_text",
		"read_value_path": false
	},
