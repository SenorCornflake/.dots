{
    "name": "neovim",
    "file_path": "~/.config/nvim/init.vim",
    "settings": {
        "colorscheme": {
            "identifier": "colorscheme .+",
            "replacer": "colorscheme <|value|>",
            "identifier_type": "line", 
            "replacer_type": "inline",
            "value_is_file": false
        },
        "termguicolors": {
            "identifier": ["\" termguicolors", "\" /termguicolors"],
            "replacer": "<|value|>",
            "identifier_type": "region", 
            "replacer_type": "inline",
            "value_is_file": false
        }
    }
}