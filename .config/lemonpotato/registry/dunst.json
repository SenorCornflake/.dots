{
    "name": "dunst",
    "file_path": "~/.config/dunst/dunstrc",
    "settings": {
        "frame_width": {
            "identifier": "frame_width = [0-9]+",
            "replacer": "frame_width = <|value|>",
            "identifier_type": "line", 
            "replacer_type": "inline",
            "value_is_file": false
        },
        "normal_frame_color": {
            "identifier": "frame_color = \"[0-9a-zA-Z#]+\" # NORMAL",
            "replacer": "frame_color = \"<|value|>\" # NORMAL",
            "identifier_type": "line", 
            "replacer_type": "inline",
            "value_is_file": false
        },
        "urgent_frame_color": {
            "identifier": "frame_color = \"[0-9a-zA-Z#]+\" # CRITICAL",
            "replacer": "frame_color = \"<|value|>\" # CRITICAL",
            "identifier_type": "line", 
            "replacer_type": "inline",
            "value_is_file": false
        },
        "normal_background": {
            "identifier": "background = \"[0-9a-zA-Z#]+\" # NORMAL",
            "replacer": "background = \"<|value|>\" # NORMAL",
            "identifier_type": "line", 
            "replacer_type": "inline",
            "value_is_file": false
        },
        "urgent_background": {
            "identifier": "background = \"[0-9a-zA-Z#]+\" # CRITICAL",
            "replacer": "background = \"<|value|>\" # CRITICAL",
            "identifier_type": "line", 
            "replacer_type": "inline",
            "value_is_file": false
        },
        "normal_foreground": {
            "identifier": "foreground = \"[0-9a-zA-Z#]+\" # NORMAL",
            "replacer": "foreground = \"<|value|>\" # NORMAL",
            "identifier_type": "line", 
            "replacer_type": "inline",
            "value_is_file": false
        },
        "urgent_foreground": {
            "identifier": "foreground = \"[0-9a-zA-Z#]+\" # CRITICAL",
            "replacer": "foreground = \"<|value|>\" # CRITICAL",
            "identifier_type": "line", 
            "replacer_type": "inline",
            "value_is_file": false
        },
        "font": {
            "identifier": "font = .+",
            "replacer": "font = <|value|>",
            "identifier_type": "line", 
            "replacer_type": "inline",
            "value_is_file": false
        },
        "icon": {
            "identifier": "icon = .+",
            "replacer": "icon = <|value|>",
            "identifier_type": "line", 
            "replacer_type": "inline",
            "value_is_file": false
        }
        
    }
}